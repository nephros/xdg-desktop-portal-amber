[Desktop Entry]
Type=Application
Name=XDG Desktop File Chooser
Exec=@CMAKE_INSTALL_FULL_LIBEXECDIR@/xdg-desktop-portal-sailfish-ui
X-Maemo-Service=org.freedesktop.impl.portal.desktop.sailfish.ui
X-Maemo-Method=org.freedesktop.impl.portal.desktop.sailfish.ui.openFilePicker
X-Maemo-Object-Path=/org/freedesktop/impl/portal/desktop/sailfish/ui
NoDisplay=true
X-DBusActivatable=true

[X-Sailjail]
Permissions=UserDirs;RemovableMedia;MediaIndexing
OrganizationName=org.freedesktop.portal.desktop.sailfish
ApplicationName=ui
ExecDBus=@CMAKE_INSTALL_FULL_LIBEXECDIR@/xdg-desktop-portal-sailfish-ui --prestart
